{
  "name": "Frictionless",
  "version": "1.3.3",
  "manifest_version": 2,
  "description":
    "Read articles on Facebook without being prompted to add apps.",
  "icons": {"48": "48.png", "128": "128.png"},
  "permissions": [
    "contextMenus",
    "cookies",
    "idle",
    "notifications",
    "unlimitedStorage",
    "http://*.facebook.com/*",
    "https://*.facebook.com/*"
  ],
  "content_scripts": [{
    "matches": ["http://*.facebook.com/*", "https://*.facebook.com/*"],
    "js": ["content.js"],
    "run_at": "document_end"
  }]
}
